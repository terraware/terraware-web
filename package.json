{
  "name": "terraware-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^2.14.0",
    "@date-io/moment": "^1.3.13",
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/sortable": "^7.0.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.88",
    "@mui/material": "^5.8.7",
    "@mui/styled-engine-sc": "^5.8.0",
    "@mui/styles": "^5.8.7",
    "@mui/x-date-pickers": "^5.0.0-alpha.7",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.23",
    "@types/mapbox-gl": "^2.3.3",
    "@types/node": "^15.12.0",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-router-dom": "^5.1.7",
    "@types/reactour": "^1.18.1",
    "axios": "^0.21.1",
    "chart.js": "^3.3.2",
    "chartjs-adapter-date-fns": "^2.0.0",
    "classnames": "^2.3.1",
    "cookies-js": "^1.2.3",
    "date-fns": "^2.28.0",
    "jwt-decode": "^3.1.2",
    "license-report": "^6.0.0",
    "mapbox-gl": "^2.4.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-localization": "^1.0.17",
    "react-map-gl": "^6.1.16",
    "react-responsive": "^9.0.0-beta.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "reactour": "https://github.com/xs9627/reactour/tarball/test",
    "recoil": "^0.3.1",
    "sass": "^1.38.0",
    "styled-components": "^5.3.5",
    "tslint-plugin-cypress": "^1.0.4",
    "typescript": "^4.3.2",
    "utm": "^1.1.1",
    "web-vitals": "^1.0.1",
    "worker-loader": "^3.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "build-with-version": "export REACT_APP_TERRAWARE_FE_BUILD_VERSION=$(git rev-parse --short HEAD) && react-scripts build",
    "build-version-file": "echo $(git rev-parse --short HEAD) > public/build-version.txt",
    "test": "TZ=America/Los_Angeles react-scripts test",
    "eject": "react-scripts eject",
    "generate-types": "node -r esm scripts/openapi-typescript-formatter.js",
    "cy:run": "TZ=UTC cypress run",
    "cy": "TZ=UTC cypress open",
    "e2e": "start-server-and-test start:dev http://localhost:3000 cy:run && yarn e2e:report",
    "e2e:report": "npx nyc report --reporter=text",
    "license-report": "license-report --only=prod --department.value=terraware --relatedTo.value=terraware --output=html > public/license-report.html",
    "lint": "tslint --project tsconfig.json",
    "ts": "tsc --project ./tsconfig.json",
    "format": "prettier --write .",
    "docker:start": "./scripts/launch_server.sh",
    "docker:stop": "docker-compose -f docker-compose.yml kill",
    "wait-be": "wait-on -t 120000 http://localhost:8080/swagger-ui/index.html",
    "server:prepare": "./scripts/preparedb.sh",
    "server:reset": "./scripts/resetdb.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.9",
    "@cypress/instrument-cra": "^1.4.0",
    "cypress": "^8.7.0",
    "esm": "^3.2.25",
    "http-proxy-middleware": "^2.0.1",
    "jest-mock-axios": "^4.4.1",
    "jest-mock-extended": "^2.0.4",
    "nyc": "^15.1.0",
    "openapi-typescript": "^4.0.2",
    "start-server-and-test": "^1.14.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2"
  }
}
