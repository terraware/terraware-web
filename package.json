    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-cypress": "^1.0.4",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^5.0.0"
  },
