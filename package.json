{
  "name": "terraware-web",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "craco start",
    "build": "craco --max_old_space_size=4096 build",
    "start:dev": "craco start",
    "build-with-version": "export REACT_APP_TERRAWARE_FE_BUILD_VERSION=$(git rev-parse --short HEAD) && craco build",
    "build-version-file": "echo $(git rev-parse --short HEAD) > public/build-version.txt",
    "test": "TZ=America/Los_Angeles craco test",
    "test-coverage": "TZ=America/Los_Angeles craco test --coverage --coverageDirectory='docs/coverage'",
    "eject": "craco eject",
    "generate-collateral": "yarn build-version-file && yarn generate-strings",
    "generate-strings": "node scripts/generate-strings.js && node scripts/unused-strings.js",
    "generate-types": "node scripts/openapi-typescript-formatter.mjs",
    "license-report": "license-report --only=prod --department.value=terraware --relatedTo.value=terraware-web --output=html > docs/license-report.html",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint -c eslint.config.js src/**/*.ts ",
    "ts": "tsc --project ./tsconfig.json",
    "format": "prettier --write .",
    "docker:start": "./scripts/launch_server.sh",
    "docker:stop": "docker compose -f docker-compose.yml kill",
    "wait-be": "wait-on -t 120000 http://localhost:8080/swagger-ui/index.html",
    "server:prepare": "./scripts/preparedb.sh",
    "server:reset": "./scripts/resetdb.sh",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "pre-commit": "yarn format && yarn lint && yarn ts && yarn test && yarn build",
    "playwright:run": "yarn run playwright test playwright/e2e",
    "playwright:debug": "yarn run playwright test playwright/e2e --debug",
    "test:e2e": "NODE_OPTIONS=--max-old-space-size=16384 start-server-and-test start http://127.0.0.1:3000 playwright:run"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/sortable": "^8.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.3",
    "@mapbox/mapbox-gl-draw": "^1.4.2",
    "@mapbox/search-js-core": "^1.0.0-beta.21",
    "@mui/base": "^5.0.0-beta.20",
    "@mui/icons-material": "^5.15.3",
    "@mui/lab": "^5.0.0-alpha.159",
    "@mui/material": "^5.15.15",
    "@mui/styled-engine-sc": "^5.14.12",
    "@mui/styles": "^5.15.15",
    "@reduxjs/toolkit": "^1.9.3",
    "@terraware/web-components": "^3.0.38",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@turf/area": "^6.5.0",
    "@turf/bbox": "^6.5.0",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/center": "^6.5.0",
    "@turf/centroid": "^6.5.0",
    "@turf/difference": "^6.5.0",
    "@turf/intersect": "^6.5.0",
    "@turf/union": "^6.5.0",
    "axios": "^1.6.3",
    "chart.js": "^4.2.0",
    "chartjs-adapter-luxon": "^1.3.1",
    "chartjs-plugin-annotation": "^3.0.1",
    "classnames": "^2.5.1",
    "cookies-js": "^1.2.3",
    "coordinate-parser": "^1.0.7",
    "date-fns": "^2.29.3",
    "hex-rgb": "^5.0.0",
    "immer": "^9.0.19",
    "is-hotkey": "^0.2.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "mapbox-gl": "^2.13.0",
    "re-reselect": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-localization": "^1.0.17",
    "react-map-gl": "^7.0.21",
    "react-mixpanel-browser": "^4.1.0",
    "react-multi-carousel": "^2.8.2",
    "react-redux": "^8.0.5",
    "react-responsive": "^9.0.0-beta.6",
    "react-router-dom": "^6.20.0",
    "slate": "^0.102.0",
    "slate-react": "^0.102.0",
    "styled-components": "^6.0.0",
    "utm": "^1.1.1",
    "web-vitals": "^3.1.1",
    "worker-loader": "^3.0.8"
  },
  "eslintConfig": {
    "extends": [
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "plugin:jest-react/recommended",
      "plugin:storybook/recommended"
    ],
    "parserOptions": {
      "project": "tsconfig.json"
    },
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-unsafe-assignment": "off",
      "@typescript-eslint/no-unsafe-member-access": "off",
      "@typescript-eslint/no-unsafe-call": "off",
      "@typescript-eslint/no-unsafe-argument": "off",
      "@typescript-eslint/no-floating-promises": "off",
      "@typescript-eslint/no-redundant-type-constituents": "off",
      "@typescript-eslint/no-unsafe-return": "off",
      "@typescript-eslint/restrict-template-expressions": "off",
      "@typescript-eslint/no-misused-promises": "off",
      "@typescript-eslint/no-base-to-string": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^axios$": "axios/dist/axios.js"
    }
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/blocks": "^7.0.12",
    "@storybook/preset-create-react-app": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/react-webpack5": "^7.0.12",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/geojson": "^7946.0.10",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.2.0",
    "@types/mapbox__mapbox-gl-draw": "^1.4.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "@typescript-eslint/parser": "^7.7.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "btoa": "^1.2.1",
    "csv": "^6.2.8",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-react": "^0.1.0",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-storybook": "^0.8.0",
    "esm": "^3.2.25",
    "express": "^4.19.2",
    "globals": "^15.0.0",
    "http-proxy-middleware": "^2.0.1",
    "jest": "^29.7.0",
    "jest-mock-axios": "^4.4.1",
    "jest-mock-extended": "^3.0.1",
    "license-report": "^6.0.0",
    "nyc": "^15.1.0",
    "openapi-typescript": "^6.7.3",
    "playwright": "^1.40.1",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "18.2.0",
    "sass": "^1.38.0",
    "start-server-and-test": "^2.0.3",
    "storybook": "^7.0.12",
    "typescript": "^5.0.0",
    "typescript-eslint": "^7.15.0",
    "webpack-dev-middleware": "^5.3.4"
  },
  "resolutions": {
    "@babel/traverse": "7.24.1",
    "fork-ts-checker-webpack-plugin": "^6.5.3",
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0",
    "strip-ansi": "6.0.1"
  }
}
