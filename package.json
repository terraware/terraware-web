    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.0.0",
    "utm": "^1.1.1",
    "web-vitals": "^3.1.1",
    "worker-loader": "^3.0.8"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
